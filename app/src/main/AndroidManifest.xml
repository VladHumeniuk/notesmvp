<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="masters.vlad.humeniuk.notesmvp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name="masters.vlad.humeniuk.notesmvp.presentation.NotesApplication"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="masters.vlad.humeniuk.notesmvp.presentation.main.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="masters.vlad.humeniuk.notesmvp.presentation.createnote.CreateNoteActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="masters.vlad.humeniuk.notesmvp.presentation.editnote.EditNoteActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="masters.vlad.humeniuk.notesmvp.presentation.createcategory.CreateCategoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="masters.vlad.humeniuk.notesmvp.presentation.editcategory.EditCategoryActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>